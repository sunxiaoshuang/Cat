
@{
    ViewData["Title"] = "达达接口测试";
}

@section Scripts {
    <script src="~/js/test/dada.js"></script>
}

<div class="mainframe" id="app">
    <div class="container-fluid">
        <div class="row">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h3 class="panel-title">
                        <span class="title">达达接口测试</span>
                    </h3>
                </div>
            </div>
            <div class="panel-body form-horizontal">
                <h3>接收订单</h3>
                <div class="form-group">
                    <label class="control-label col-xs-2 col-md-1  col-xs-offset-1">
                        <span class="require">订单编号：</span>
                    </label>
                    <div class="col-xs-4 col-md-3">
                        <input type="text" class="form-control" v-model="orderCode" />
                    </div>
                    <div class="col-xs-1">
                        <div class="kui-button">
                            <button type="button" class="btn btn-primary" v-on:click="receive()">
                                确定
                            </button>
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-xs-12">
                        <div class="kui-button">
                            <button type="button" class="btn btn-primary" v-on:click="receive()">
                                接收
                            </button>
                        </div>
                    </div>
                    <div class="col-xs-12">
                        <div class="kui-button">
                            <button type="button" class="btn btn-primary" v-on:click="fetch()">
                                取货
                            </button>
                        </div>
                    </div>
                    <div class="col-xs-12">
                        <div class="kui-button">
                            <button type="button" class="btn btn-primary" v-on:click="finish()">
                                完成
                            </button>
                        </div>
                    </div>
                    <div class="col-xs-12">
                        <div class="kui-button">
                            <button type="button" class="btn btn-primary" v-on:click="cancel()">
                                取消
                            </button>
                        </div>
                    </div>
                    <div class="col-xs-12">
                        <div class="kui-button">
                            <button type="button" class="btn btn-primary" v-on:click="expire()">
                                过期
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
