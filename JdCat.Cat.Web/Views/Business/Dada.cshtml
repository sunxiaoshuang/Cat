
@{
    ViewData["Title"] = "Dada";
}


<div class="mainframe" id="app">
    <div class="container-fluid">
        <div class="row">
            <div class="panel panel-default">
                <div class="panel-heading">
                    <h3 class="panel-title">
                        <span class="title">达达配送设置</span>
                    </h3>
                </div>
            </div>
            <div class="panel-body form-horizontal">
                <div class="form-group" v-bind:class="{'has-error': (showError && !entity.cityCode)}">
                    <label class="control-label col-xs-2 col-md-1  col-xs-offset-1">
                        <span class="require">所属城市</span>
                    </label>
                    <div class=" col-xs-4 col-md-3">
                        <select class="form-control" v-model="entity.cityCode">
                            <option v-for="city in cityList" :value="city.code">{{city.name}}</option>
                        </select>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

